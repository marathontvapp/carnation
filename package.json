{
  "name": "carnation-project",
  "version": "1.0.0-alpha.0",
  "private": true,
  "workspaces": [
    "docs",
    "packages/*",
    "examples/*"
  ],
  "release-it": {
    "plugins": {
      "@release-it-plugins/workspaces": true
    },
    "npm": false
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "release": "release-it"
  },
  "devDependencies": {
    "@release-it-plugins/workspaces": "^3.2.0",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "release-it": "^15.10.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
